<One>:
    MDToolbar:
        title:"AI-Face"
        pos_hint:{"top":1}
        left_action_items:[["face-recognition"]]
        right_action_items:[["human"]]
        elevation:20
    MDIcon:
        icon:"face-recognition"
        pos_hint: {"center_x": .5, "center_y": .7}
        halign:"center"
        font_size:150

    MDLabel:
        text:"Artificial-Intelligence Algorithms"
        halign:"center"
        font_size:'20dp'
        bold: True

    MDFillRoundFlatButton:
        text:"Start"
        pos_hint:{"center_x":0.5,"center_y":0.4}
        on_press: 
            root.manager.current = "two"
            root.manager.transition.direction = 'left'


    MDBottomAppBar:
        MDToolbar:
            title:""
            type:"bottom"
            left_action_items:[["human-handsdown"]]
            mode:"center"
            icon: 'hand'
            on_action_button: app.dialog()

